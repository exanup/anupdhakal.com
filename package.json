{
  "version": "1.0.0",
  "private": true,
  "name": "anupdhakal.me",
  "description": "Personal blog by Anup Dhakal",
  "homepage": "https://anupdhakal.me",
  "author": "Anup Dhakal <anupdhakal2000@gmail.com>",
  "repository": "https://github.com/exanup/anupdhakal.me",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exanup/anupdhakal.me/issues"
  },
  "engines": {
    "node": "14.15"
  },
  "scripts": {
    "develop": "gatsby develop",
    "develop-clean": "yarn run clean && gatsby develop",
    "build": "gatsby build",
    "build-clean": "yarn run clean && gatsby build",
    "serve": "gatsby serve",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy-clean": "yarn run clean && gatsby build --prefix-paths && gh-pages -d public",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "lint:js": "eslint --cache --ext .js,.jsx .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\" \"yarn flow\"",
    "test": "jest --config ./jest/jest-config.js",
    "test:coverage": "jest --coverage --config ./jest/jest-config.js",
    "test:watch": "jest --watch --config ./jest/jest-config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx}": [
      "yarn lint:scss",
      "yarn lint:js",
      "yarn flow",
      "git add"
    ],
    "src/**/*.{scss}": [
      "yarn lint:scss",
      "git add"
    ]
  },
  "dependencies": {
    "@sentry/gatsby": "^6.7.1",
    "@sentry/tracing": "^5.29.2",
    "bluebird": "^3.7.2",
    "classnames": "^2.2.6",
    "codecov": "^3.8.2",
    "gatsby": "^3.7.2",
    "gatsby-link": "^3.7.1",
    "gatsby-plugin-catch-links": "^3.7.1",
    "gatsby-plugin-feed": "^3.7.1",
    "gatsby-plugin-flow": "^2.7.1",
    "gatsby-plugin-google-gtag": "^3.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-netlify": "^3.7.1",
    "gatsby-plugin-netlify-cms": "^5.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-optimize-svgs": "^1.0.5",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-webfonts": "^2.1.0",
    "gatsby-remark-autolink-headers": "^4.4.1",
    "gatsby-remark-copy-linked-files": "^4.4.1",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-remark-katex": "^5.4.1",
    "gatsby-remark-prismjs": "^5.4.1",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^4.4.1",
    "gatsby-remark-smartypants": "^4.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "invariant": "^2.2.4",
    "katex": "^0.12.0",
    "lodash": "^4.17.21",
    "netlify-cms-app": "^2.14.12",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.2",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/preset-env": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "autoprefixer": "9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "1.7.1",
    "browserslist": "4.16.1",
    "concurrently": "6.2.0",
    "eslint": "7.17.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-watch": "7.0.0",
    "flow-bin": "^0.142.0",
    "flow-typed": "^3.3.1",
    "gh-pages": "3.1.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "lint-staged": "^10.5.4",
    "lost": "8.3.1",
    "postcss-pxtorem": "5.1.1",
    "react-test-renderer": "17.0.2",
    "rimraf": "3.0.2",
    "stylelint": "13.8.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.19.0"
  }
}
